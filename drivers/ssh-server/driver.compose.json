{
  "name": {
    "en": "ssh-server"
  },
  "class": "other",
  "capabilities": [
    "alarm_connection_failed",
    "last_connected"
  ],
  "platforms": [
    "local"
  ],
  "connectivity": [
    "lan"
  ],
  "images": {
    "small": "{{driverAssetsPath}}/images/small.png",
    "large": "{{driverAssetsPath}}/images/large.png",
    "xlarge": "{{driverAssetsPath}}/images/xlarge.png"
  },
  "pair": [
    {
      "id": "connect",
      "navigation": {
        "next": "list_my_devices"
      }
    },
    {
      "id": "list_my_devices",
      "template": "list_devices",
      "navigation": {
        "next": "add_my_devices",
        "prev": "connect"
      },
      "options": {
        "singular": true
      }
    },
    {
      "id": "add_my_devices",
      "template": "add_devices"
    }
  ]
}
