        "hint": {
          "en": "IP address or host name of the SSH server",
          "de": "IP Adresse oder Hostname des SSH Servers"
        }
      },
      {
        "id": "port",
        "type": "number",
        "label": {
          "en": "Server port",
          "de": "Server port"
